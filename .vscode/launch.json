{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Launch Program",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}/packages/backend-adapter/dist/adapters/AbstractTaskAdapter.d.ts",
			"outFiles": ["${workspaceFolder}/**/*.js"]
		},
		{
			"type": "node-terminal",

			"name": "Run Script: bootstrap",
			"request": "launch",
			"command": "yarn run bootstrap",
			"cwd": "${workspaceFolder}"
		}
	]
}
